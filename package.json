    "react-twitter-widgets": "^1.7.1",
    "tailwindcss": "^2.0.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "cli-glob": "^0.1.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.20",
    "prettier": "^2.0.0",
    "remark-cli": "^11.0.0",
    "remark-preset-lint-recommended": "^6.0.0",
    "stylefmt": "^6.0.3",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "write-good": "^1.0.8"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "not dead"
  ],
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "scripts": {
    "preload-fonts": "gatsby-preload-fonts",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/blog/",
    "write-good": "write-good $(glob 'content/blog/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "format:styles": "prettier '**/*.{css,scss}' --write",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "overrides": {
    "graphql@>14.7.0": "^16.0.0",
    "gatsby@<5.0.0": "^5.0.0",
    "browserslist": "^4.0.0"
  }
}
