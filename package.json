  "keywords": [
    "gatsby",
    "shaderpixel"
  ],
  "license": "MIT",
  "engines": {
    "node": "^18.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "gatsby": "^5.0.0",
    "gatsby-cli": "^5.0.0",
    "gatsby-plugin-catch-links": "^5.0.0",
    "gatsby-plugin-emotion": "^8.0.0",
    "gatsby-plugin-feed": "^5.0.0",
    "gatsby-plugin-google-analytics": "^5.0.0",
    "gatsby-plugin-image": "^3.0.0",
