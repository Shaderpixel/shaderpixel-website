    "gatsby-plugin-manifest": "^5.0.0",
    "gatsby-plugin-netlify": "^5.1.0",
    "gatsby-plugin-nprogress": "^5.0.0",
    "gatsby-plugin-offline": "^6.0.0",
    "gatsby-plugin-postcss": "^6.0.0",
    "gatsby-plugin-preload-fonts": "^4.0.0",
    "gatsby-plugin-purgecss": "^6.0.0",
