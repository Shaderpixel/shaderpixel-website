    "postcss-preset-env": "^7.0.0",
    "prismjs": "^1.29.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-headroom": "^3.2.1",
    "react-helmet": "^6.0.0",
    "react-share": "^4.1.0",
